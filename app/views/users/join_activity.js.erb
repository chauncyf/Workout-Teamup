<% if @join_status %>
  refreshPosts();
  new PNotify({
      title: '<i class="fas fa-check-circle"></i> Congratulation',
      text: 'Activity joined',
      type: 'success'
  });
<% else %>
  new PNotify({
      title: '<i class="fas fa-exclamation-circle"></i> Opps',
      text: 'Activity already joined',
      type: 'error'
  });
<% end %>


